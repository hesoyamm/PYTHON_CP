   "source": [
    "#cyclic rotation of an array\n",
    "#i.e. move an array by K value\n",
    "def rotate_array_by_k_times(k,array):\n",
    "    while(k!=0):\n",
    "        k=k-1;\n",
    "        array.insert(0,array[-1])\n",
    "        array.pop()\n",
    "    return array"
   ]
